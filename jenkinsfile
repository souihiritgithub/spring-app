pipeline {
    agent any
    
    stages {
        stage('Clone') {
            steps {
                sh "rm -rf *"
                sh "git clone https://github.com/souihiritgithub/spring-app.git"
            }
        }
        
        stage('Compile') {
            steps {
                sh '/usr/bin/mvn compile'
            }
        }    
    }
}
