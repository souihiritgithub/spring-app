pipeline {
    agent any
    
    stages {
        stage('Clone') {
            steps {
                sh 'git clone https://github.com/souihiritgithub/spring-app.git'
            }
        }
        
        stage('compile') {
            steps {
                sh 'mvn compile '
            }
        }
        stage('Package') {
            steps {
                sh 'mvn package'
            }
        }
    }
}
