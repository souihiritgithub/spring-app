pipeline {
    agent any
    
    stages {
        stage('Clone') {
            steps {
                sh "git clone https://github.com/souihiritgithub/spring-app.git"
            }
        }
        
        stage('Build and Package') {
            steps {
                sh 'mvn compile'
            }
        }
        
        stage('Build') {
            steps {
                echo 'This is a new pipeline.'
            }
        }
    }
}
