pipeline {
    agent any
    
    stages {
        stage('Clone') {
            steps {
                sh "rm -rf *"
                sh "git clone https://github.com/souihiritgithub/spring-app.git"
            }
        }
        
        stage('Build and Package') {
            steps {
                sh 'mvn clean package'
            }
        }
    
        
    }
}
